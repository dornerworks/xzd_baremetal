		};

		nand@ff100000 {
			compatible = "arasan,nfc-v3p10";
			status = "disabled";
			reg = <0x0 0xff100000 0x1000>;
			clock-names = "clk_sys", "clk_flash";
			interrupt-parent = <0x1>;
			interrupts = <0x0 0xe 0x4>;
			#address-cells = <0x2>;
			#size-cells = <0x1>;
			clocks = <0x2 0x2>;
		};

		gem0: ethernet@ff0b0000 {
			compatible = "cdns,gem";
			status = "disabled";
			interrupt-parent = <0x1>;
			interrupts = <0x0 0x39 0x4 0x0 0x39 0x4>;
			reg = <0x0 0xff0b0000 0x1000>;
			clock-names = "pclk", "hclk", "tx_clk";
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			jumbo-max-len = <0x2800>;
			jumbo-supported;
			clocks = <0x3 0x3 0x3>;
		};

		gem1: ethernet@ff0c0000 {
			compatible = "cdns,gem";
			status = "disabled";
			interrupt-parent = <0x1>;
			interrupts = <0x0 0x3b 0x4 0x0 0x3b 0x4>;
			reg = <0x0 0xff0c0000 0x1000>;
			clock-names = "pclk", "hclk", "tx_clk";
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			jumbo-max-len = <0x2800>;
			jumbo-supported;
			clocks = <0x3 0x3 0x3>;
		};

		gem2: ethernet@ff0d0000 {
			compatible = "cdns,gem";
			status = "disabled";
			interrupt-parent = <0x1>;
			interrupts = <0x0 0x3d 0x4 0x0 0x3d 0x4>;
			reg = <0x0 0xff0d0000 0x1000>;
			clock-names = "pclk", "hclk", "tx_clk";
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			jumbo-max-len = <0x2800>;
			jumbo-supported;
			clocks = <0x3 0x3 0x3>;
		};

		gem3: ethernet@ff0e0000 {
                        #stream-id-cells = <0x1>;
			compatible = "cdns,gem";
			status = "disabled";
			interrupt-parent = <0x1>;
			interrupts = <0x0 0x3f 0x4 0x0 0x3f 0x4>;
			reg = <0x0 0xff0e0000 0x1000>;
			clock-names = "pclk", "hclk", "tx_clk";
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			jumbo-max-len = <0x2800>;
			jumbo-supported;
			clocks = <0x3 0x3 0x3>;
			phy-mode = "rgmii-id";
			xlnx,ptp-enet-clock = <0x0>;
			local-mac-address = [00 0a 35 00 22 01];
			phy-handle = <0x4>;
                        xen,passthrough = <1>;

			phy@21 {
				reg = <0x15>;
				ti,rx-internal-delay = <0x8>;
				ti,tx-internal-delay = <0xa>;
				ti,fifo-depth = <0x1>;
				linux,phandle = <0x4>;
				phandle = <0x4>;
			};
		};

		gpio@ff0a0000 {
			compatible = "xlnx,zynqmp-gpio-1.0";
			status = "okay";
			#gpio-cells = <0x2>;
			interrupt-parent = <0x1>;
			interrupts = <0x0 0x10 0x4>;
			reg = <0x0 0xff0a0000 0x1000>;
			clocks = <0x2>;
			emio-gpio-width = <0x20>;
			gpio-mask-high = <0x0>;
			gpio-mask-low = <0x5600>;
		};

		i2c@ff020000 {
			compatible = "cdns,i2c-r1p10";
			status = "okay";
			interrupt-parent = <0x1>;
			interrupts = <0x0 0x11 0x4>;
			reg = <0x0 0xff020000 0x1000>;
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			clocks = <0x2>;
			clock-frequency = <0x61a80>;

			i2cswitch@75 {
				compatible = "nxp,pca9544";
				#address-cells = <0x1>;
				#size-cells = <0x0>;
				reg = <0x75>;

				i2c@0 {
					#address-cells = <0x1>;
					#size-cells = <0x0>;
					reg = <0x0>;

					ina226@40 {
						compatible = "ti,ina226";
						reg = <0x40>;
						shunt-resistor = <0x1388>;
					};

					ina226@41 {
						compatible = "ti,ina226";
						reg = <0x41>;
						shunt-resistor = <0x1388>;
					};

					ina226@42 {
						compatible = "ti,ina226";
						reg = <0x42>;
						shunt-resistor = <0x1388>;
					};

					ina226@43 {
						compatible = "ti,ina226";
