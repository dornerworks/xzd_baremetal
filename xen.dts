			interrupt-parent = <0x1>;
			interrupts = <0x0 0x7f 0x4>;
			xlnx,id = <0x3>;
			xlnx,bus-width = <0x80>;
		};

		dma@fd540000 {
			status = "okay";
			compatible = "xlnx,zynqmp-dma-1.0";
			reg = <0x0 0xfd540000 0x1000>;
			interrupt-parent = <0x1>;
			interrupts = <0x0 0x80 0x4>;
			xlnx,id = <0x4>;
			xlnx,bus-width = <0x80>;
