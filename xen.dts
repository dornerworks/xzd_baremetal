			reg = <0x0 0xfd560000 0x1000>;
			interrupt-parent = <0x1>;
			interrupts = <0x0 0x82 0x4>;
			xlnx,id = <0x6>;
			xlnx,bus-width = <0x80>;
		};

		dma@fd570000 {
			status = "okay";
			compatible = "xlnx,zynqmp-dma-1.0";
			reg = <0x0 0xfd570000 0x1000>;
			interrupt-parent = <0x1>;
			interrupts = <0x0 0x83 0x4>;
			xlnx,id = <0x7>;
			xlnx,bus-width = <0x80>;
		};

		gpu@fd4b0000 {
			status = "okay";
			compatible = "arm,mali-400", "arm,mali-utgard";
			reg = <0x0 0xfd4b0000 0x30000>;
			interrupt-parent = <0x1>;
			interrupts = <0x0 0x84 0x4 0x0 0x84 0x4 0x0 0x84 0x4 0x0 0x84 0x4 0x0 0x84 0x4 0x0 0x84 0x4>;
			interrupt-names = "IRQGP", "IRQGPMMU", "IRQPP0", "IRQPPMMU0", "IRQPP1", "IRQPPMMU1";
		};

		dma@ffa80000 {
			status = "disabled";
			compatible = "xlnx,zynqmp-dma-1.0";
			reg = <0x0 0xffa80000 0x1000>;
			interrupt-parent = <0x1>;
			interrupts = <0x0 0x4d 0x4>;
			xlnx,id = <0x0>;
			xlnx,bus-width = <0x40>;
		};

		dma@ffa90000 {
			status = "disabled";
			compatible = "xlnx,zynqmp-dma-1.0";
			reg = <0x0 0xffa90000 0x1000>;
			interrupt-parent = <0x1>;
			interrupts = <0x0 0x4e 0x4>;
			xlnx,id = <0x1>;
			xlnx,bus-width = <0x40>;
		};

		dma@ffaa0000 {
			status = "disabled";
			compatible = "xlnx,zynqmp-dma-1.0";
			reg = <0x0 0xffaa0000 0x1000>;
			interrupt-parent = <0x1>;
			interrupts = <0x0 0x4f 0x4>;
			xlnx,id = <0x2>;
			xlnx,bus-width = <0x40>;
		};

		dma@ffab0000 {
			status = "disabled";
			compatible = "xlnx,zynqmp-dma-1.0";
			reg = <0x0 0xffab0000 0x1000>;
			interrupt-parent = <0x1>;
			interrupts = <0x0 0x50 0x4>;
			xlnx,id = <0x3>;
			xlnx,bus-width = <0x40>;
		};

		dma@ffac0000 {
			status = "disabled";
			compatible = "xlnx,zynqmp-dma-1.0";
			reg = <0x0 0xffac0000 0x1000>;
			interrupt-parent = <0x1>;
			interrupts = <0x0 0x51 0x4>;
			xlnx,id = <0x4>;
			xlnx,bus-width = <0x40>;
		};

		dma@ffad0000 {
			status = "disabled";
			compatible = "xlnx,zynqmp-dma-1.0";
			reg = <0x0 0xffad0000 0x1000>;
			interrupt-parent = <0x1>;
			interrupts = <0x0 0x52 0x4>;
